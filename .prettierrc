printWidth: 120
proseWrap: always
singleQuote: true
semi: true
bracketSpacing: true
parser: babel
trailingComma: all
overrides:
- files:
  - '*.ts'
  - '*.tsx'
  options:
    parser: typescript
- files: '*.md'
  options:
    parser: markdown
- files: '*.json'
  options:
    parser: json
    printWidth: 80
- files: '*.css'
  options:
    parser: css
